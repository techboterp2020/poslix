<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
     <record id="product_product_form_inherited" model="ir.ui.view">
        <field name="name">product.product.form</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_tag_ids']" position="after">
                <field name="free_qty" string="Salable Quantity"/>
            </xpath>
        </field>
 </record>
<record id="product_template_form_inherited" model="ir.ui.view">
        <field name="name">product.template.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_tag_ids']" position="after">
                <field name="salable_qty"/>
            </xpath>
        </field>
 </record>

</odoo>